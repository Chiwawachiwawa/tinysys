	"version": "0.2.0",
    "configurations": [
	    {
            "name": "C++ Launch",
            "type": "cppdbg",
            "request": "launch",
            "program": "${workspaceFolder}/samples/hellodebugger/hellodebugger.elf",
            "args": [],
            "stopAtEntry": false,
			"stopAtConnect": true,
            "cwd": "${workspaceFolder}/",
            "environment": [],
            "externalConsole": false,
            "MIMode": "gdb",
			"useExtendedRemote": true,
            "setupCommands": [
                {
                    "description": "Enable pretty-printing for gdb",
                    "text": "-enable-pretty-printing",
                    "ignoreFailures": true
                }
            ],
            "miDebuggerPath": "riscv64-unknown-elf-gdb",
            "miDebuggerServerAddress": "//.//COM9"
        }
    ]
}