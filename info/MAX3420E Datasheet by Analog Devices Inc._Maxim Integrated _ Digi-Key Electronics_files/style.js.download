// Lan 3
$(document).ready(function()
{   
    // get sizeBase:
    var heightDsContainer0 = $("#datasheet-page-container").height();
    var sizePageContainer0 = $(".sub-page-container").width();
    var sizePage0 = $("#pf1").width();
    
    // get scale
    var scalePage = sizePageContainer0 / sizePage0;
    if(scalePage < 1)
    {
        $(".sub-page-container").css("transform", "scale(" + scalePage + ")");
        
        // Fix height
        $("#datasheet-page-container").height(heightDsContainer0 * scalePage);
        //$("#datasheet-page-container").height(10748.229604);
        //$("#datasheet-page-container").css("height", "10748px");
    }
    
    $(window).resize(function()
    {
        var sizePageContainer1 = $(".sub-page-container").width();
    
        scalePage = sizePageContainer1 / sizePage0;
        if(scalePage < 1)
        {
            $(".sub-page-container").css("transform", "scale(" + scalePage + ")");
        
            // Fix height
            $("#datasheet-page-container").height(heightDsContainer0 * scalePage);
        }
        else
        {
            $(".sub-page-container").css("transform", "scale(1)");
        
            // Fix height
            $("#datasheet-page-container").height(heightDsContainer0);
        }
        
    });
});